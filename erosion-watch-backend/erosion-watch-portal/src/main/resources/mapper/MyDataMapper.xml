<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.nwafu.erosion.portal.mapper.MyDataMapper">
    <insert id="insert" parameterType="cn.edu.nwafu.erosion.portal.domain.entity.ExcelFile">
        INSERT INTO excel_file
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="fileName != null">
                file_name,
            </if>
            <if test="author != null">
                author,
            </if>
            <if test="createTime != null">
                create_time,
            </if>
            <if test="updateTime != null">
                update_time,
            </if>
            <if test="updateUser != null">
                update_user,
            </if>
            <if test="urlPath != null">
                url_path,
            </if>
            <if test="mongoId != null">
                mongo_id,
            </if>
        </trim>
        <trim prefix="VALUES (" suffix=")" suffixOverrides=",">
            <if test="fileName != null">
                #{fileName},
            </if>
            <if test="author != null">
                #{author},
            </if>
            <if test="createTime != null">
                #{createTime},
            </if>
            <if test="updateTime != null">
                #{updateTime},
            </if>
            <if test="updateUser != null">
                #{updateUser},
            </if>
            <if test="urlPath != null">
                #{urlPath},
            </if>
            <if test="mongoId != null">
                #{mongoId},
            </if>
        </trim>
    </insert>

</mapper>
